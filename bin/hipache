#!/usr/bin/env node

'use strict';

var program = require('commander'),
    hipache = require('../lib/hipache')

program
  .version('0.2.4')
  .option('-c, --config [file]', 'Location of the configuration file')
  .parse(process.argv);

if (!program.config) program.help();

Hipache = hipache.run(program.config)
